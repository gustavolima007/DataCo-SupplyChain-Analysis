	partition Dim_Calendario = calculated
		mode: import
		source =
				
				VAR DataMinima = MIN('DataCoSupplyChainDataset'[order date (DateOrders)])
				VAR DataMaxima = MAX('DataCoSupplyChainDataset'[shipping date (DateOrders)])
				VAR DataInicial = DATE(YEAR(DataMinima), 1, 1)
				VAR DataFinal = DATE(YEAR(DataMaxima), 12, 31)
				RETURN
				ADDCOLUMNS (
				    CALENDAR (DataInicial, DataFinal),
				    "Ano", YEAR ( [Date] ),
				    "Mês Num", MONTH ( [Date] ),
				    "Mês Nome", FORMAT ( [Date], "MMMM", "pt-BR" ),
				    "Mês Abrev", UPPER( FORMAT ( [Date], "MMM", "pt-BR" ) ),
				    "Mês/Ano", FORMAT ( [Date], "MM/yyyy" ),
				    "Mês/Ano Sort", YEAR ( [Date] ) * 100 + MONTH ( [Date] ),
				    "Ano/Mês", FORMAT([Date], "YYYY/MM")
				)

	annotation PBI_Id = d278e9b78de14aea92b74a412aeb5a34

